{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2017", "es2015", "dom", "es6"
    ],
    "module": "commonjs",
    "outDir": "lib",
    "sourceMap": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noImplicitAny": true
  },
  "include": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
